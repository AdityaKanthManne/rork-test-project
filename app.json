{
  "expo": {
    "name": "Splitwise",
    "slug": "splitwise-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/assets/images/logo.svg",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./public/assets/images/logo.svg",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.splitwise.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/assets/images/logo.svg",
        "backgroundColor": "#000000"
      },
      "package": "com.splitwise.app"
    },
    "web": {
      "favicon": "./public/assets/images/favicon.ico",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}